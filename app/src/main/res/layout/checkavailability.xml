<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/white"
    tools:context=".CheckAvailability">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#0FFFFFFF"
        android:minHeight="?attr/actionBarSize"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textviewCheckAvailability"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginVertical="18dp"
        android:text="Check Availability"
        android:textColor="#1975AE"
        android:textSize="21sp"
        android:textStyle="bold"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar1"
        app:layout_constraintEnd_toEndOf="@+id/toolbar1"
        app:layout_constraintStart_toStartOf="@+id/toolbar1"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/backBttn_CheckAvailability"
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/ic_roundback"
        android:scaleType="fitCenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />


    <TextView
        android:id="@+id/textView10"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:gravity="center"
        android:text="See your location to check if our services has reached your area!"
        android:textColor="#000000"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar1"
        tools:ignore="HardcodedText" />

    <FrameLayout
        android:id="@+id/frameLayoutMainServer"
        android:layout_width="350dp"
        android:layout_height="130dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="16dp"
        android:background="@drawable/corner_solidwhite"
        app:layout_constraintEnd_toEndOf="@+id/textView10"
        app:layout_constraintStart_toStartOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/textView10">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            android:text="@string/main_server"
            android:textColor="#000000"
            android:textStyle="bold">

        </TextView>

        <TextView
            android:layout_width="210dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="40dp"
            android:text="@string/mainserverLocsServer"
            android:textColor="#000000"
            tools:ignore="TextSizeCheck">

        </TextView>

        <Button
            android:id="@+id/applynowMainServerBttn"
            android:layout_width="110dp"
            android:layout_height="35dp"
            android:layout_marginStart="225dp"
            android:layout_marginTop="80dp"
            android:background="@drawable/corner_solidblue"
            android:onClick="OpenMainServer"
            android:text="Apply Now"
            android:textColor="@color/white"
            android:textSize="12sp"
            tools:ignore="DuplicateSpeakableTextCheck,HardcodedText,TextSizeCheck,TouchTargetSizeCheck">

        </Button>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayoutSanSebastianServer"
        android:layout_width="350dp"
        android:layout_height="130dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="16dp"
        android:background="@drawable/corner_solidwhite"
        app:layout_constraintEnd_toEndOf="@+id/frameLayoutMainServer"
        app:layout_constraintStart_toStartOf="@+id/frameLayoutMainServer"
        app:layout_constraintTop_toBottomOf="@+id/frameLayoutMainServer">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            android:text="@string/san_sebastian_server"
            android:textColor="#000000"
            android:textStyle="bold">

        </TextView>

        <TextView
            android:layout_width="210dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="40dp"
            android:text="@string/sansebastianLocsServer"
            android:textColor="#000000"
            tools:ignore="TextSizeCheck">

        </TextView>

        <Button
            android:id="@+id/applynowSanSebastianServerBttn"
            android:layout_width="110dp"
            android:layout_height="35dp"
            android:layout_marginStart="225dp"
            android:layout_marginTop="80dp"
            android:background="@drawable/corner_solidblue"
            android:onClick="OpenSanSebServer"
            android:text="Applynow"
            android:textColor="@color/white"
            android:textSize="12sp"
            tools:ignore="DuplicateSpeakableTextCheck,HardcodedText,TextSizeCheck,TouchTargetSizeCheck">

        </Button>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayoutSanIsidroServer"
        android:layout_width="350dp"
        android:layout_height="130dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="16dp"
        android:background="@drawable/corner_solidwhite"
        app:layout_constraintEnd_toEndOf="@+id/frameLayoutSanSebastianServer"
        app:layout_constraintStart_toStartOf="@+id/frameLayoutSanSebastianServer"
        app:layout_constraintTop_toBottomOf="@+id/frameLayoutSanSebastianServer">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            android:text="@string/san_isidro_server"
            android:textColor="#000000"
            android:textStyle="bold">

        </TextView>

        <TextView
            android:layout_width="210dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="40dp"
            android:text="@string/sanisidroLocsServer"
            android:textColor="#000000"
            tools:ignore="TextSizeCheck">

        </TextView>

        <Button
            android:id="@+id/applynowSanIsidroServerBttn"
            android:layout_width="110dp"
            android:layout_height="35dp"
            android:layout_marginStart="225dp"
            android:layout_marginTop="80dp"
            android:background="@drawable/corner_solidblue"
            android:onClick="OpenSanIsidroServer"
            android:text="Applynow"
            android:textColor="@color/white"
            android:textSize="12sp"
            tools:ignore="DuplicateSpeakableTextCheck,HardcodedText,TextSizeCheck,TouchTargetSizeCheck">

        </Button>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayoutPalapatServer"
        android:layout_width="350dp"
        android:layout_height="130dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="16dp"
        android:background="@drawable/corner_solidwhite"
        app:layout_constraintEnd_toEndOf="@+id/frameLayoutSanIsidroServer"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/frameLayoutSanIsidroServer"
        app:layout_constraintTop_toBottomOf="@+id/frameLayoutSanIsidroServer">


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            android:text="@string/palapat_server"
            android:textColor="#000000"
            android:textStyle="bold">

        </TextView>

        <TextView
            android:layout_width="210dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="40dp"
            android:text="@string/palapatLocsServer"
            android:textColor="#000000"
            tools:ignore="TextSizeCheck">

        </TextView>

        <Button
            android:id="@+id/applynowPalapatServerBttn"
            android:layout_width="110dp"
            android:layout_height="35dp"
            android:layout_marginStart="225dp"
            android:layout_marginTop="80dp"
            android:background="@drawable/corner_solidblue"
            android:onClick="OpenPalapatServer"
            android:text="Applynow"
            android:textColor="@color/white"
            android:textSize="12sp"
            tools:ignore="DuplicateSpeakableTextCheck,HardcodedText,TextSizeCheck,TouchTargetSizeCheck">

        </Button>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>